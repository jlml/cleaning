<div id="page" class="grid">
<table class = "table">
	<tr>
		<td>OrderID</td>
		<td>Cleaner</td> 
		<td>Number</td>
		<td>Duration</td>
		<td>Time</td>
		<td>Rooms</td>
	</tr>
	<%@user.orders.each do |order|%>
		<tr>
			<td><%= order.id%></td>
			<td><%= order.cleaner%></td> 
			<td><%= order.number%></td>
			<td><%= order.cleanduration%></td>
			<td><%= order.cleantime%></td>
			<td><%= order.rooms%></td>
		</tr>
	<%end%>
</table>
</div>